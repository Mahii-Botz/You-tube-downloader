{
  "name": "mahii-modz-youtube-downloader",
  "version": "2.0.0",
  "description": "MAHII-MODZ Professional YouTube Video Downloader with modern web interface",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "install-deps": "npm install && npm run install-ytdlp",
    "install-ytdlp": "echo 'Please install yt-dlp: pip install yt-dlp'",
    "test": "node -e \"console.log('Testing server...'); require('./server.js')\"",
    "clean": "rm -rf downloads/*.* && echo 'Downloads folder cleaned'",
    "health": "curl http://localhost:3000/api/health || echo 'Server not running'"
  },
  "keywords": [
    "youtube",
    "downloader",
    "video",
    "audio",
    "express",
    "nodejs",
    "yt-dlp",
    "mahii-modz",
    "streaming",
    "converter"
  ],
  "author": "MAHII-MODZ",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mahii-modz/youtube-downloader"
  },
  "bugs": {
    "url": "https://github.com/mahii-modz/youtube-downloader/issues"
  },
  "homepage": "https://github.com/mahii-modz/youtube-downloader#readme"
